  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ventai/windsurf-enterprise-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/ventai/windsurf-enterprise-mcp/issues"
  },
  "homepage": "https://github.com/ventai/windsurf-enterprise-mcp#readme"
