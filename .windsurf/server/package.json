{
  "name": "@windsurf/enterprise-mcp-server",
  "version": "2.0.0",
  "description": "Windsurf Enterprise MCP Server with Vector Store, Graph Relations & PostgreSQL/Redis Integration",
  "main": "dist/enterprise-index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/enterprise-index.js",
    "dev": "tsx src/enterprise-index.ts",
    "start:vector": "node dist/enterprise-index.js",
    "test": "jest",
    "test:vector": "npm run test -- --testPathPattern=vector",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t windsurf-enterprise-mcp .",
    "docker:run": "docker run --env-file .env windsurf-enterprise-mcp"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "diff": "^5.1.0",
    "minimatch": "^9.0.3",
    "redis": "^5.0.1",
    "pg": "^8.11.3",
    "openai": "^4.24.0",
    "@types/pg": "^8.10.9",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/diff": "^5.0.8",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "keywords": [
    "mcp", 
    "filesystem", 
    "windsurf", 
    "ventai", 
    "vector-store", 
    "graph-database", 
    "postgresql", 
    "redis", 
    "enterprise"
  ],
