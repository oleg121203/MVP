{
  "name": "ventai-enterprise",
  "version": "2.0.0",
  "description": "VentAI Enterprise - AI-powered HVAC platform with advanced analytics",
  "scripts": {
    "dev:backend": "cd backend && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:enterprise": "./start-enterprise-services.sh && npm run dev",
    "enterprise:services": "./start-enterprise-services.sh",
    "enterprise:migrate": "cd backend && python enterprise_migration.py",
    "enterprise:setup": "npm run enterprise:services && npm run enterprise:migrate",
    "dashboard:dev": "cd frontend && REACT_APP_MODE=dashboard npm start",
    "test:enterprise": "cd backend && pytest tests/enterprise/",
    "test:performance": "cd backend && pytest tests/performance/",
    "build:frontend": "cd frontend && npm run build",
    "build:enterprise": "cd frontend && REACT_APP_MODE=enterprise npm run build",
    "start:prod": "concurrently \"cd backend && uvicorn main:app --host 0.0.0.0 --port 8000\" \"cd frontend && npm start\"",
    "analytics:dev": "cd backend && python -m analytics.main",
    "ai:test": "cd backend && python -m ai.test_providers",
    "test": "jest",
    "test:frontend": "jest tests/frontend",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest-integration.config.js",
    "build": "react-scripts build",
    "start:server": "ts-node server.ts",
    "dev:server": "nodemon server.ts",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "build": "tsc",
    "docker:build": "docker build -f deploy/Dockerfile.prod -t ventai-analytics .",
    "docker:run": "docker run -p 8000:8000 ventai-analytics",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/msgpack-lite": "^0.1.11",
    "@types/redis": "^4.0.10",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "redis": "^5.5.6",
    "ts-jest": "^29.1.1"
  },
  "keywords": [
    "ai",
    "hvac",
    "enterprise",
    "analytics",
    "automation"
  ],
  "author": "VentAI Enterprise Team",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "msgpack-lite": "^0.1.26",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "react-window": "^1.8.11",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "typescript": "^5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
